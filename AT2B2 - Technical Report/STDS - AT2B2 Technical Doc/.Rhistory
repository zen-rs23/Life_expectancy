scale_fill_viridis(discrete=TRUE, guide=FALSE, option="A") +
theme_ipsum() +
theme(legend.position="bottom")
# Trying diff plots
p <- ggplot(life, aes(factor(Status), Life_expectancy))
p + geom_violin()
p + geom_boxplot(outlier.colour="black", outlier.shape=16,
outlier.size=2, notch=FALSE)
bxp <-p+geom_violin()
dp <- p+geom_boxplot(outlier.colour="black", outlier.shape=16,
outlier.size=2, notch=FALSE)
ggarrange(bxp, dp,
labels = c("Violin", "Box"),
ncol = 2, nrow = 2)
